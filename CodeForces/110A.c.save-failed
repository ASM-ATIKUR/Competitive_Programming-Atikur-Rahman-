#include <stdio.h>
#include <string.h>

int main()
{
    int i, j, count=0, allcount=0, digit;
    long long int n;
    scanf("%d", &n);

    while(n>0)
    {
        digit=n%10;
        if(digit==4 || digit==7) count++;
        n/=10;
        allcount++;
    }
    if(count==allcount) printf("YES\n");


}
